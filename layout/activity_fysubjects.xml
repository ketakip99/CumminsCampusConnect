        android:gravity="center"
        android:outlineAmbientShadowColor="#FFFFFF"
        android:outlineSpotShadowColor="#FFFFFF"
        android:text="FIRST YEAR"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textColorHint="#F3F3F3"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.12" />

    <TextView

        android:id="@+id/textView"
        android:layout_width="187dp"
        android:layout_height="63dp"
        android:gravity="center"
        android:text="Semester I"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333" />
    <TextView
        android:id="@+id/textView3"
        android:layout_width="187dp"
        android:layout_height="63dp"
        android:gravity="center"
        android:text="Semester II"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.747" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="267dp"
        android:layout_height="75dp"
        android:gravity="center"
        android:text="_______________________"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.6" />
    <Button
        android:id="@+id/button"
        android:layout_width="273dp"
        android:layout_height="57dp"
        android:background="#02155A"
        android:backgroundTint="#02155A"
        android:text="Syllabus and Structure"

        android:textSize="20sp"
        app:cornerRadius="0sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.461"
        app:strokeColor="#FFFFFF"
        app:strokeWidth="2dp" />
    <Button
        android:id="@+id/button2"
        android:layout_width="270dp"
        android:layout_height="53dp"
        android:background="#02155A"
        android:backgroundTint="#02155A"
        android:text="Syllabus and Structure"
        android:textSize="20sp"
        app:cornerRadius="0sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.524"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.864" />
